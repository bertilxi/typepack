    "target": "es5",
    "module": "commonjs",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "outDir": "dist",
    "strict": true,
    "sourceMap": true,
    "noImplicitAny": false,
    "types": ["node"]
